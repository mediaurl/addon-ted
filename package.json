{
  "name": "mediaurl-ted",
  "version": "0.0.0",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "develop": "ts-node-dev --transpile-only --no-notify src"
  },
  "dependencies": {
    "@mediaurl/sdk": "^1.0.1",
    "cheerio": "0.22.0",
    "qs": "6.9.4"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.23",
    "@types/qs": "6.9.5",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
