{
  "name": "watched-addon-ted",
  "version": "0.0.0",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "develop": "ts-node-dev --transpile-only --no-notify src"
  },
  "dependencies": {
    "@watchedcom/sdk": "^0.35.1",
    "cheerio": "^1.0.0-rc.3",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/qs": "^6.9.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
