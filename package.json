{
  "name": "mediaurl-ted",
  "version": "0.0.0",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "develop": "ts-node-dev --transpile-only --no-notify src"
  },
  "dependencies": {
    "@mediaurl/sdk": "^2.0.0",
    "cheerio": "0.22.0",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/qs": "^6.9.7",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
